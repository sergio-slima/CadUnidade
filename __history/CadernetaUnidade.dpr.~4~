program CadernetaUnidade;

uses
  System.StartUpCopy,
  FMX.Forms,
  UInicio in 'UInicio.pas' {FormInicio},
  ULogin in 'ULogin.pas' {FormLogin};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TFormInicio, FormInicio);
  Application.CreateForm(TFormLogin, FormLogin);
  Application.Run;
end.
